generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("LOGDB_URL")
}

model command_log {
  id       Int     @id @default(autoincrement())
  user     Decimal @db.Decimal
  command  String
  channel  String
  options  Json?   @db.Json
  jsonData Json
}

model team_notes {
  id         Int      @id @default(autoincrement())
  user       String
  noterId    Decimal  @db.Decimal
  noterName  String
  notes      String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
