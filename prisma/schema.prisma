generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("LOGDB_URL")
}

model command_log {
  id         Int      @id @default(autoincrement())
  user       Decimal  @db.Decimal
  command    String
  channel    String
  options    Json?    @db.Json
  jsonData   Json
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}

model team_notes {
  id         Int      @id @default(autoincrement())
  user       String
  noterId    Decimal  @db.Decimal
  noterName  String
  note       String
  display    Boolean  @default(true)
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}

model society_finance {
  id         Int      @id @default(autoincrement())
  job        String
  label      String
  bank       Decimal  @db.Decimal
  money      Decimal  @db.Decimal
  black      Decimal  @db.Decimal
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
